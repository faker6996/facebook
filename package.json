{
  "name": "attendance-system-fe-main",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--inspect next dev",
    "dev_v2": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "docker:local-up": "docker-compose -f docker-compose.yml -f docker-compose.local.yml --env-file .env.local up -d",
    "docker:down": "docker-compose down",
    "docker:restart": "npm run docker:down && npm run docker:up",
    "docker:dev-up": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml --env-file .env.dev up -d",
    "docker:dev-down": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml down",
    "docker:prod-up": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml --env-file .env up -d",
    "docker:prod-down": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml down"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.7.0",
    "@tailwindcss/postcss": "^4.1.7",
    "axios": "^1.9.0",
    "bcrypt": "^6.0.0",
    "clsx": "^2.1.1",
    "cookie": "^1.0.2",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "next": "15.3.1",
    "next-intl": "^4.1.0",
    "pg": "^8.16.0",
    "postcss": "^8.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-redux": "^9.2.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^4.1.7"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie": "^0.6.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^20",
    "@types/pg": "^8.15.4",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^7.0.3",
    "typescript": "^5"
  }
}
